{
  "name": "mmse.ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch",
    "testify": "jest --watch --noStackTrace",
    "test:cov": "jest --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "npm run build:continuous",
    "build:prod": "ng build --prod --aot --no-sourcemap --output-hashing=none",
    "build:continuous": "ng build --watch --output-path ../mmse/Scripts/Dev",
    "copy": "xcopy \"dist\" \"..\\mmse\\Scripts\\Dev\" /i /s /r /y /c",
    "copy:prod": "xcopy \"dist\" \"..\\mmse\\Scripts\\Prod\" /i /s /r /y /c",
    "dist": "npm run build && npm run copy",
    "dist:prod": "npm run build:prod && npm run copy:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.0",
    "@angular/cdk": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/flex-layout": "2.0.0-beta.12",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@tinymce/tinymce-angular": "^1.0.5",
    "@types/jest": "^22.0.1",
    "core-js": "^2.4.1",
    "jest": "^22.1.1",
    "jest-localstorage-mock": "^2.2.0",
    "jest-preset-angular": "^5.0.0",
    "material-components-web": "^0.28.0",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.5",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@compodoc/compodoc": "^1.0.5",
    "@types/jasmine": "^2.8.6",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "<rootDir>/src/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setup-jest.ts",
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "coverageReporters": [
      "text",
      "text-summary"
    ]
  }
}
